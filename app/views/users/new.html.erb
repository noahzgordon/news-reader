<%= form_errors %>

<form action="<%= users_url %>" method="post">
  <%= auth_token %>

  <label for="username">Username</label>
  <input
    id="username"
    type="text"
    name="user[username]"
    value="<%= @user.username %>">

  <label for="password">Password</label>
  <input
    id="password"
    type="password"
    name="user[password]">

  <button>Sign Up</button>

</form>